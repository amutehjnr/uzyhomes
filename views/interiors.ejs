                        <ul>
                            <li>Procurement and logistics</li>
                            <li>Installation supervision</li>
                            <li>Final styling and photography</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Services - New Layout -->
